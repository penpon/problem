/* Flexbox基礎の最小実装 */
.flex-container {
  display: flex;
}
.justify-center {
  justify-content: center;
}
.justify-between {
  justify-content: space-between;
}
.align-center {
  align-items: center;
  height: 120px;
}
.direction-column {
  flex-direction: column;
  height: 200px;
  width: 200px;
  margin: 0 auto;
}